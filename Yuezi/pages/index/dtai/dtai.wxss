/* dtai.wxss */
.dtai-list .container{
  display: flex;
  border: 0;
  align-items: center;
  justify-content: space-between;
  margin: 20rpx 0;
  
}
.dtai-list .container image{
  width: 30rpx;
  height: 30rpx;
}
.dtai-list .container text{
  font-size: 30rpx;
  font-weight: 500;
  align-self: center;
  flex: auto;
  margin-left: 10rpx;
}
.prevCE{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.prevCE image{
  width: 216rpx;
  height: 145rpx;
}